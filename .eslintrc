{
	"extends": "eslint:recommended",
	"root": true,
	"env": {
		"browser": true
	},
	"rules": {
		"block-scoped-var": 2,
		"brace-style": [2, "1tbs"],
		"camelcase": 2,
		"comma-dangle": [2, "never"],
		"comma-spacing": [2, { "before": false, "after": true }],
		"comma-style": [2, "last"],
		"curly": 2,
		"default-case": 2,
		"dot-notation": 2,
		"eol-last": 2,
		"eqeqeq": [2, "smart"],
		"guard-for-in": 2,
		"keyword-spacing": 2,
		"max-depth": [1, 3],
		"new-cap": 1,
		"no-caller": 2,
		"no-eq-null": 2,
		"no-eval": 2,
		"no-implied-eval": 2,
		"no-extend-native": 2,
		"no-fallthrough": 2,
		"no-irregular-whitespace": 0,
		"no-labels": 2,
		"no-mixed-spaces-and-tabs": 2,
		"no-multi-spaces": 2,
		"no-native-reassign": 2,
		"no-new-wrappers": 2,
		"no-octal": 2,
		"no-redeclare": 2,
		"no-return-assign": 2,
		"no-self-compare": 2,
		"no-shadow-restricted-names": 2,
		"no-trailing-spaces": 2,
		"no-use-before-define": [2, "nofunc"],
		"no-underscore-dangle": 2,
		"no-unused-vars": 2,
		"no-undef-init": 2,
		"no-undef": 2,
		"no-with": 2,
		"one-var": [2, "never"],
		"quotes": [2, "single"],
		"radix": 2,
		"semi": [2, "always"],
		"space-unary-ops": 2,
		"strict": [2, "function"]
	}
}
